  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/0xVaibhav11/avatarkit/issues"
  },
  "homepage": "https://github.com/0xVaibhav11/avatarkit",
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
